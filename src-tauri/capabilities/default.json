{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Enables default permissions for Devora",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-message",
    "dialog:allow-ask",
    "opener:default",
    "updater:default",
    "process:allow-restart"
  ]
}
