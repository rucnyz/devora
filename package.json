{
  "name": "devora",
  "version": "0.1.4",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "concurrently -k \"bun run dev:server\" \"bun run dev:client\"",
    "dev:server": "bun run --watch server/index.ts",
    "dev:client": "vite",
    "build": "vite build",
    "start": "bun run server/index.ts",
    "preview": "bun run build && bun run start",
    "release": "bun run scripts/build-release.ts",
    "release:windows": "bun run scripts/build-release.ts windows-x64",
    "release:macos": "bun run scripts/build-release.ts macos-arm64",
    "release:linux": "bun run scripts/build-release.ts linux-x64",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "lint": "eslint src server",
    "lint:fix": "eslint src server --fix",
    "version": "bun run scripts/version.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@happy-dom/global-registrator": "^20.0.11",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/react": "^16.3.1",
    "@types/bun": "latest",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^11.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "happy-dom": "^20.0.11",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript-eslint": "^8.52.0",
    "vite": "^7.3.0"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@uiw/react-md-editor": "^4.0.11",
    "hono": "^4.11.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0",
    "uuid": "^13.0.0"
  }
}
